(def element CompendiumSecondaryTabSampleLayout ()
	(scope
		(var imageTabList:array = "[{ url: 'url:../service_kit/currencies/icon_exp.png',				count: 5 },
									{ url: 'url:../service_kit/currencies/icon_coal.png',				count: 10 },
									{ url: 'url:../service_kit/currencies/icon_santium.png',			count: 15 },
									{ url: 'url:../service_kit/currencies/icon_recruitment_points.png',	count: 20 },
									{ url: 'url:../service_kit/currencies/icon_bpPoints.png',			count: 25 }]")

		(event evTabSelected)
		(var selectedItem:number = 0)
		(bind selectedItem "$event.index" init=false (event "evTabSelected"))

		(event ItemChooser_MarkerTypeDropdownClicked)
		(var selectedMarkerIndex:number = 0)
		(bind selectedMarkerIndex "$event.selectedIndex" init=false (event "ItemChooser_MarkerTypeDropdownClicked"))

		(var selectedMarkerType:number = "COMPENDIUM_SIMPLE_TAB_CONTENT_MARKER_TYPE[selectedMarkerIndex].value")

		(event evIsEnabledChanged)
		(var isEnabled:bool = true)
		(bind isEnabled "!isEnabled" init=false watch=false (event "evIsEnabledChanged"))

		(event evHasImagesChanged)
		(var hasImages:bool = false)
		(bind hasImages "!hasImages" init=false watch=false (event "evHasImagesChanged"))

		(event evHasCounterChanged)
		(var hasCounter:bool = false)
		(bind hasCounter "!hasCounter" init=false watch=false (event "evHasCounterChanged"))
	)

	(element CompendiumElementTitle
		_title = 'TabButton'
	)

	(block
		(class $CompendiumColtrolsContainer)

		(hblock
			(controller $Repeat renderer='TabButton'
				(count = "imageTabList.length")
				(args
					_label = "hasCounter ? toString(imageTabList[$index].count) : 'Tab ' + toString($index + 1)"
					_width = 125px
					_isDisabled = "!isEnabled"
					_image = "hasImages ? imageTabList[$index].url : ''"
					_isSelected = "selectedItem == $index"
					_dispatchedEv = "'evTabSelected'"
					_dispatchParams = "{ index: $index }"
					_isNewMarker = "selectedMarkerType == SC.Ui_common.CONTENT_MARKER_TYPE.NEW"
					_isAttentionMarker = "selectedMarkerType == SC.Ui_common.CONTENT_MARKER_TYPE.ATTENTION"
					_newContentCounter = "selectedMarkerType == SC.Ui_common.CONTENT_MARKER_TYPE.NEW_COUNT ? 99 : 0"
				)
			)

			(block
				(style
					(position = "absolute")
					(width = 100%)
					(bottom = 0)
				)

				(element HorizontalDividerTwoPx)
			)
		)

		(block
			(style
				(marginTop = "SXS")
				(vgap = "SXS")
			)

			(hblock
				(style (align = "middle"))

				(tf
					(class $TextDefault14NM)
					(style
						(alpha = "TA")
						(marginRight = "S")
					)
					(text = 'MarkerType:')
				)
	
				(element ItemChooser
					_items = "COMPENDIUM_SIMPLE_TAB_CONTENT_MARKER_TYPE"
					_listItemRenderer = 'MarkerTypeItemRenderer'
					_choosedItemRenderer = 'MarkerTypeChooserItemRenderer'
					_onItemSelectedEvent = 'MarkerTypeDropdownClicked'
					_selectedMaxWidth = 200
					_dropDownWidth = 150
					_maxDropDownHeight = 200
					_curIndex = "selectedMarkerIndex"
				)
			)

			(element SwitchWithRightLabel
				_label = 'isEnabled'
				_state = "isEnabled"
				_dispatchedEv = 'evIsEnabledChanged'
			)

			(element SwitchWithRightLabel
				_label = 'hasImages'
				_state = "hasImages"
				_dispatchedEv = 'evHasImagesChanged'
			)

			(element SwitchWithRightLabel
				_label = 'hasCounter'
				_state = "hasCounter"
				_dispatchedEv = 'evHasCounterChanged'
			)
		)
	)
)
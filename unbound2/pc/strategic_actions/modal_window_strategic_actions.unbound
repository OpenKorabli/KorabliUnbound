		(var thresholdTaskIds:str = "'IDS_SSE_CONDITION_CATEGORY_DESCRIPTION_' + toUpper(conditionCategory) + '_FULL'")

		(var taskDescription:str = "_isThresholdTask ? thresholdTaskIds : taskDescriptionData.ids")

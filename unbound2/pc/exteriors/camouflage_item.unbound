		(var isDisablePreviewAction:bool = "_previewExteriorId && !(_previewExteriorId == _exteriorEntity.ownCamouflage.id)")

		(var isPreviewed:bool = "(_previewExteriorId == _exteriorEntity.ownCamouflage.id) && exteriorsInStorage == 0")

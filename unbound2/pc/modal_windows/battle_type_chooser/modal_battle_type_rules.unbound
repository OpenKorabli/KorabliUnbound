(def element ModalWindowBattleTypeRules (_selectedBattleTypeId:number)
	(macro MODAL_WINDOW_INIT)
	(scope
		(event evCarouselScrollValueChanged)
		(event evBattletypeRulesChanged)

		(struct stageSize = STAGE_SIZE())

		(var isSmallScreen:bool = "stageSize.width < 1550px")

		
		(var sortedChooserBattleTypes:dhCollection = "getCollectionByPath(CC.battleType, 'forChooser.sortedForChooser')")
		(var battleTypeEntity:dhEntity = "getEntity(_selectedBattleTypeId) ?: sortedChooserBattleTypes.firstEntity")
		(bind battleTypeEntity "getEntity($event._selectedBattletypeSideTabId)" init=false watch=false (event "evBattletypeRulesChanged"))

		
		(var eventBattleStateComponent:dhComponent = "battleTypeEntity.eventBattleState")
		(var eventBattleName:str = "eventBattleStateComponent.name" (event "eventBattleStateComponent.evChanged"))

		
		(var operationComponent:dhComponent = "battleTypeEntity.operation")
		(var customOperationName:str = "operationComponent.customIconName")
		(var operationName:str = "customOperationName ?: operationComponent.gpName")

		
		(var battleType:str = "battleTypeEntity.battleType.type")
		(var isEventBattle:bool = "battleType == SC.Common.BATTLE_TYPES.EVENT_BATTLE")
		(var isPveBattle:bool = "battleType == SC.Common.BATTLE_TYPES.PVE_BATTLE")
		(var rulesName:str = "	isEventBattle	?	eventBattleName in RULES	?	eventBattleName
																				:	SC.Common.BATTLE_TYPES.EVENT_BATTLE :
								isPveBattle		?	operationName in RULES		?	operationName
																				:	SC.Common.BATTLE_TYPES.PVE_BATTLE
												:	battleType")
		(var rulesCount:number = "RULES[rulesName].length")

		
		(var rulesBgName:str = "rulesName in BG_CONFIG_BATTLE_RULES		? rulesName :
								battleType in BG_CONFIG_BATTLE_RULES	? battleType
																		: 'Default'")

		
		(var battleSeasonInfoComponent:dhComponent = "battleTypeEntity.battleSeasonInfo")
		(var achievementsIDs:array = "battleSeasonInfoComponent.achievementsUI ?: []")
		(var hasAchievementsToShow:bool = "achievementsIDs.length > 0")

		
		(var battletypeIconName:str = "	eventBattleName	?	eventBattleName :
										operationName	?	operationName
														:	battleType")
		(var battletypeIconDir:str = "'url:../service_kit/battle_types/' + battletypeIconName + '_rules.png'")
	)

	(class $Fullsize)
	(style (align = "center"))

	(name = 'ModalWindowBattleTypeRules')

	
	(block
		(class $FullsizeAbsolute)
		(element ModalWindowAnimatedBackground
			_selectedCategory = "rulesBgName"
			_type = "SC.Ui_styles.ANIMATED_BG.BATTLE_RULES"
			_hideTime = 0.15
			_hideDelay = 0.3
		)
	)

	
	(block
		(macro DEFAULT_MODAL_WINDOW_ANIMATION 1)
		(class $FullsizeAbsolute)
		(style
			(align = "bottom|right")
			(paddingRight = 100px)
			(paddingBottom = 35px)
		)

		(block
			(style
				(backgroundSize = "cover")
				(alpha = 0.16)
				(bind height "isSmallScreen ? 250px : 500px")
				(bind width "isSmallScreen ? 250px : 500px")
				(bind backgroundImage "battletypeIconDir")
			)
		)
	)

	
	(block
		(macro DEFAULT_MODAL_WINDOW_ANIMATION 1)
		(style (width = 100%))

		(element ModalWindowHeaderFullSize
			_windowName = 'IDS_MODAL_WINDOW_BATTLETYPE_RULES'
			_paddingRight = "M"
			_paddingLeft = "M"
		)
	)

	
	(block
		(macro DEFAULT_MODAL_WINDOW_ANIMATION 1)
		(style
			(position = "absolute")
			(height = 100%)
			(paddingTop = "{ 720:130px, 1080:225px }")
			(marginLeft = "{ 1280:XXS, 1920:M }")
		)

		(element BattletypeRulesTabButtons
			_selectedBattletypeId = "battleTypeEntity.id"
		)
	)

	(block
		(macro DEFAULT_MODAL_WINDOW_ANIMATION 1)
		(class $Fullsize)
		(style
			(paddingLeft = "{ 1280:350px, 1920:488px }")
			(paddingTop = "{ 720:MS, 1080:XL }")
			(paddingBottom = "{ 720:MS, 1080:LS }")
		)

		(block
			(style
				(width = 100%)
				(marginBottom = "{ 720:MS, 1080:LM }")
				(paddingLeft = "XS")
			)

			(tf
				(class $TextDefaultBold40NM)
				(style (alpha = "TA"))
				(bind text "toUpper(tr('IDS_MODAL_WINDOW_' + rulesName + '_RULES_HEADER'))")
			)
		)

		(hblock
			(class $Fullsize)

			(block
				(style
					(height = 100%)
					(bind marginRight "isSmallScreen ? XL : XLM")
					(bind width "hasAchievementsToShow	?	isSmallScreen	?	400px
					 														:	512px
														:	100%")
				)

				(scrollArea
					(class $Fullsize)

					(macro DEFAULT_VERTICAL_SCROLL_PARAMS
						_singleStep = "150px"
						_wheelScrollAcceleration = "0.8"
					)

					(content
						(style
							(width = 100%)
							(padding = "XS")
							(vgap = "{ 720:LS, 1080:XL }")
						)

						(controller $Repeat renderer='RulesCardTextItem' name='rulesCardContainer'
							(bind items "RULES[rulesName]")
							(args
								_rulesName = "rulesName"
								_rulesList = "$item"
							)
						)
					)
					(bindcall scrollToDirection horDir="ScrollDirection.LEFT" vertDir="ScrollDirection.TOP"	animated="false" on='addedToStage')
				)
			)

			(block
				(bind visible "hasAchievementsToShow")
				(style
					(height = 100%)
					(padding = "XXS")
					(bind width "isSmallScreen ? 400px : 512px")
				)

				(tf
					(class $TextDefaultBold30NM)
					(style
						(marginBottom = "MS")
						(alpha = "TA")
					)
					(text = 'IDS_MODAL_WINDOW_RULES_ACHIEVEMENT_HEADER')
				)

				(block
					(style
						(width = 100%)
						(marginBottom = "L")
					)

					(element RepeatParagraphItemNM
						_paragraphText = 'IDS_GET_ACHIEVEMENTS_IN'
						_width = 100%
						_alpha = "TC"
						_size = "SIZE.MEDIUM"
					)
				)

				(htile
					(style
						(width = 100%)
						(hgap = "L")
					)

					(controller $Repeat renderer='AchievementIcon'
						(bind enabled "hasAchievementsToShow")
						(bind count "achievementsIDs.length")
						(args
							_id = "toString(achievementsIDs[$index])"
							_size = 80px
						)
					)
				)
			)
		)
	)
)

(def element RulesCardTextItem (_rulesName:str, _rulesList:dict)
	(scope
		(struct stageSize = STAGE_SIZE())
		(var rulesTextTypeList:array = "_rulesList._textTypes ?: []")
		(var rulesParagraphText:str = "'IDS_MODAL_WINDOW_' + _rulesName + '_RULES_PARAGRAPH_' + tr($index + 1)")
	)

	(style (width = 100%))

	(tf
		(bind class "stageSize.height < 800 ? '$TextDefaultBold26NM' : '$TextDefaultBold30NM'")
		(style
			(width = 100%)
			(marginBottom = "MS")
			(alpha = "TA")
		)
		(bind text "rulesParagraphText + '_HEADER'")
	)

	(block
		(style
			(width = 100%)
			(vgap = "M")
		)

		(controller $Repeat renderer='RulesCardTextLine'
			(bind items "rulesTextTypeList")
			(args
				_rulesTextType = "$item._type"
				_rulesArgs = "$item._args"
				_rulesParagraphText = "rulesParagraphText + '_TEXT_' + tr($index + 1)"
			)
		)
	)
)

(def element RulesCardTextLine (_rulesTextType:number, _rulesParagraphText:str, _rulesArgs:dict={})
	(style (width = 100%))

	(controller $Instance renderer='RepeatParagraphItemNM'
		(bind enabled "_rulesTextType == SC.Ui_common.MODAL_RULES_TEXT_TYPE.PARAGRAPH")
		(args
			_paragraphText = "_rulesParagraphText"
			_width = 100%
			_alpha = "TC"
			_size = "SIZE.MEDIUM"
		)
	)

	(controller $Instance renderer = 'RepeatRestriction'
		(bind enabled "_rulesTextType == SC.Ui_common.MODAL_RULES_TEXT_TYPE.RESTRICTION")
		(args
			_restrictionData = "_rulesArgs"
			_paragraphText = "_rulesParagraphText"
		)
	)
)
				(align = "center|middle")
				(paddingTop = 3px)
				(paddingBottom = 3px)
				(paddingLeft = 6px)
				(paddingRight = 6px)
				(hitTest = false)
			)

			(element ShipImage
				_shipId = "_rewardItem.id"
				_size = "SC.Ui_styles.SIZE.SMALL"
				_hasWater = true
				_hasSmoke = true
			)
		)

		
		(block
			(style (position = "absolute") (right = "XXS") (top = "XXS"))
			(controller $Instance renderer='RentTimerIcon'
				(bind enabled "isRentShip")
				(args
					_rentUntil = "rentUntil"
					_rentDuration = "rentDuration"
				)
			)
		)
		
		(block

		)

		(block
			(bind visible "isRentShip && !_isRentHidden")
			(style
				(position = "absolute") (left = 4px) (bottom = 7px) (hitTest = false)
				(width = 21px) (height = 21px) (backgroundImage = "'url:../service_kit/icons/icon_rent_white.png'")
			)
		)
	)


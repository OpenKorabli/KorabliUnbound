(def element RewardCategory (_rewardCategory:str, _isReceived:bool, _isUnavailable:bool = false, _isRewardActive:bool = true)
	(block
		(bind visible "_isRewardActive")
		(class $MiddleVHAbsolutely)
		(style
			(hitTest = false)
			(backgroundSize = "cover")
			(width = 124px) (height = 124px)
			(bind backgroundImage "_isRewardActive ? 'url:../animations/spine/reward_states/available.skel' : ''")
		)
		(controller $Spine (bind enabled "_isRewardActive"))
	)
	(block
		(style
			(width = 48px) (height = 48px)
			(bind backgroundImage "'url:../reward_categories/' + _rewardCategory + '.png'")
			(bind alpha "_isReceived || _isUnavailable ? 0.5 : 1")
		)
	)
	(block
		(bind visible "_isReceived")
		(class $MiddleAlignedAbsolutely)
		(style
			(width = 48px) (height = 48px) (hitTest = false)
			(backgroundImage = "'url:../reward_categories/icon_check.png'")
		)
	)
)

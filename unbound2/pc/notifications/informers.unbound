(def constant COMMON_LOG_WIDTH 446px)

(def constant ACHIEVEMENT_LOG_WIDTH 344px)
(def constant ACHIEVEMENT_LOG_HEIGHT 96px)

(def constant ACHIEVEMENT_LOG_DELAY_SHOW 0.46)
(def constant ACHIEVEMENT_LOG_DURATION_SHOW 0.453)

(def constant COMMON_LOG_DURATION_SHOW 0.34)

(def constant CUSTOM_INFORMER_BACKGROUND_LIST {	SC.System_messages.SYS_MESSAGE_TYPE_UI.ACHIEVEMENT_EARNED_INFORMER				: 'AchievementInfotipBg'})

(def constant COMMON_INFORMER_CONTENT_LIST {	SC.System_messages.SYS_MESSAGE_TYPE_UI.CLAN_BATTLE_PRIMETIME_CHANGED			: 'InformerClanBattlesPrimetime',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.PREMUIM_DAY_LEFT							: 'InformerPremDayLeft',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.CLAN_RULES_HAVE_CHANGED					: 'InformerClanBattlesPrimetime',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.RANK_BATTLE_STATE_CHANGED				: 'InformerRankBattles',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.BRAWL_BATTLES_STATE_CHANGED				: 'InformerBrawlBattles',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.MISSING_EXPENDABLE_ITEMS_IN_SHIP_BUILD	: 'InformerExpendablesExpired',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.REPLACE_EXPENDABLE_ITEMS_IN_SHIP_BUILD	: 'InformerExpendablesReplaced',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.NEWBIE_QUEST_INFORMER					: 'InformerNewbieQuest',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.PARAGON_SEASON_INFORMER					: 'InformerParagonsSeason',
												SC.System_messages.SYS_MESSAGE_TYPE_UI.RATING_BATTLES_INFORMER					: 'InformerRatingBattlesSeason'})

(def constant CUSTOM_INFORMER_CONTENT_LIST {	SC.System_messages.SYS_MESSAGE_TYPE_UI.ACHIEVEMENT_EARNED_INFORMER				: 'InformerAchievementEarned'})

(def element InformerLogContainer ()
	(scope
		(var renderedSystemMessageCollection:dhCollection = "getCollectionByPath(CC.renderedSysMsg, 'byClassId.' + CC.informerSysMsgToBeRendered)")
	)

	(controller $Repeat renderer = 'InformerLogNotificationItem'
		(bind items "renderedSystemMessageCollection")
		(args
			_informerEntity = "$item"
		)
	)
)

(def element AchievementInfotipBg (_rollOver:bool)
	(scope
		(event evBgAnimEnded)
		(macro TOOLTIP_SYSTEM_DEFAULT_BACKGROUND_SCOPE)
	)
	(style
		(width = "ACHIEVEMENT_LOG_WIDTH")
		(height = "ACHIEVEMENT_LOG_HEIGHT")
		(position = "absolute")
		(align = "center")
	)

	(block
		(class $Fullsize)
		(style
			(pivotX = 50%)
			(ubScaleX = 0)
			(alpha = 0)
		)

		(controller $Animation
			(bindcall play
				delay =		"ACHIEVEMENT_LOG_DELAY_SHOW"
				duration =	"ACHIEVEMENT_LOG_DURATION_SHOW"
				to =		"{ alpha: 1, ubScaleX: 1}"
				easing =	"Easing.expo_in_out"
				action =	"kill"
				on =		'addedToStage'
			)
			(dispatch evBgAnimEnded dir="EventDirection.UP" on=evAnimEnded)
		)

		(block
			(dispatch 'evHideMessage' dir="EventDirection.UP" on='click')

			(class $FullsizeAbsolute)
			(style (backgroundColor = "NO_COLOR"))
			(macro TOOLTIP_SYSTEM_DEFAULT_BACKGROUND_MARKUP)

			(controller $Animation
				(bindcall play
					duration =	0.1
					to =		"_rollOver ? { alpha: 1.5 } : { alpha: 1 }"
					easing =	"Easing.sine_in"
					watch = false
					action =	"kill"
					(bind trigger "_rollOver")
				)
			)
		)
	)

	(block
		(class $MiddleAlignedAbsolutely)
		(style
			(hitTest = false)
			(width = 560px)
			(height = 176px)
			(top = -40px)
			(backgroundImage = 'url:../animations/spine/rewards/achievement_effect/achievement_effect.skel')
			(backgroundSize = "fill")
		)

		(controller $Spine
			(bindcall play sequence="{ name:'show',		repeatCount:1			}" append=true	on='addedToStage')
			(bindcall play sequence="{ name:'static',	repeatCount:-1,	mix:0.3	}" append=true	on='addedToStage')
		)
	)
)

(def element CommonInfotipBg (_rollOver:bool)
	(scope
		(event evBgAnimEnded)
		(macro TOOLTIP_SYSTEM_DEFAULT_BACKGROUND_SCOPE)
	)
	(class $FullsizeAbsolute)
	(style (align = "center"))

	(block
		(class $Fullsize)
		(style
			(pivotX = 50%)

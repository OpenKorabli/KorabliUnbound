(def element InformerRatingBattlesSeason (_data:dict)
	(scope
		(macro SERVER_TIME_SCOPE)
		(macro COUNTDOWN_SCOPE "'timeLeft'" "_data.finishTime" "'HIGHEST,WITH_DAYS'")
	)
	(bindcall externalCall 'direct.action' "['ratingBattles.setInformerSeen', [_data.messageType]]" watch=false on='addedToStage')

	(element CommonInformer
		_iconUrl = 'url:../service_kit/battle_types/RatingBattle.png'
		_textItems = "['IDS_RATING_BATTLES_INFORMER_TYPE_' + _data.messageType]"
		_subtitleText = "_data.messageType == SC.Common.RATING_BATTLES_INFORMERS_TYPES.END_SEASON	? subst('IDS_INFORMER_TIME_LEFT', [], {_timeLeft: timeLeft})
																									: null"
	)
)

(def element InformerAchievementEarned (_data:dict, _isInformerVisible:bool)
	(scope
		(var achievementEntity:dhEntity = "getEntity(_data.achievementId)")
		(var achievementComponent:dhComponent = "achievementEntity.achievement")
		(var achievementInfo:dhComponent = "achievementEntity.achievementInfo")

		(var achievementName:str = "achievementInfo.name ?: ''")
		(var nameIDS:str = "achievementInfo.nameIDS")
		(var rarity:number = "achievementInfo.rarity")
		(var typeIDS:str = "rarity ? 'IDS_ACHIEVEMENT_RARITY_' + SC.Common.UNIFIED_ITEM_RARITY.VALUE_TO_NAME[rarity] : ''")
	)
	(bindcall externalCall 'sound.playSetSoundDirect' "['achievement_earned', 'start']" on='addedToStage')

	(style
		(hitTest = false)
		(width = "ACHIEVEMENT_LOG_WIDTH")
		(height = "ACHIEVEMENT_LOG_HEIGHT")
	)

	(block
		(class $Fullsize)
		(style
			(flow = horizontal)
			(align = "middle")
			(paddingTop = "S")
			(paddingBottom = "S")
			(paddingLeft = "M")
			(paddingRight = "L")
		)

		(block
			(style (marginRight = "SXS"))
			(element AchievementBattleIcon
				_iconName = "achievementName"
				_size = 80px
			)
		)

		(block
			(class $Fullsize)
			(style
				(align = "middle")
				(vgap="S")
			)

			(tf
				(class $TextDefaultBold18NM)
				(style
					(width = 100%)
					(maxHeight = 75px)
					(elideMode = true)
					(wordWrap = true)
					(alpha = "TA")
				)
				(bind text "nameIDS")
			)

			(tf
				(class $TextDefaultNM)
				(style
					(width = 100%)
					(alpha = "TA")
				)
				(bind text "typeIDS")
			)
		)
	)
)

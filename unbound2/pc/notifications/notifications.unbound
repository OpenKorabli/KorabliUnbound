		)
	)

	(block
		(style
			(paddingLeft = 6px)
			(paddingRight = 7px)
			(paddingTop = 5px)
			(paddingBottom = 6px)
		)

		(tf
			(class $TextDefaultBold18NM)
			(style (alpha = "ITEM_CHOOSER_LABEL_ALPHA[_state]"))
			(bind text "tr('IDS_' + toUpper(_item))")

			(macro DEFAULT_CONTROL_STATE_ANIMATION_ALPHA
				_trigger = "_state"
				_alpha = "ITEM_CHOOSER_LABEL_ALPHA[_state]"
			)
		)
	)
)


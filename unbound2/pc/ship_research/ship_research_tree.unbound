			)
		)
		(block
			(style (position = "absolute") (width = 100%) (top = "-XXS"))
			(element HorizontalDividerTwoPx)
		)
	)

	
	(block
		(style (position = "absolute") (left = 0px) (bind top "stageHeight * 0.11") (marginTop = "{ 720:LS, 1080:XXS }"))
		

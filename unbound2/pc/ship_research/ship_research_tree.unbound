		(style (width = 100%) (height = 37px) (hitTest = false))

		(block
			(style
				(width = 100%)
				(height = 24px)
				(alpha = 0.2)
				(backgroundImage = 'url:../service_kit/panel_backgrounds/drag_shadow.png')
				(scale9grid = 3)

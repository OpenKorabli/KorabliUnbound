(def element TOOLTIP_SYSTEM_DEFAULT_BACKGROUND_MARKUP_UB1 (_isInfotip:bool=false, _isHighContrast:bool=false)
	(class $Fullsize)
	(style (bind hitTest "_isInfotip"))

	(element DeclareBlurLayer)

	(block
		(class $FullsizeAbsolute)
		(controller $Instance renderer='BlurMapRoundedPanel'
			(bind enabled "!_isHighContrast")
		)
	)

	(block
		(bind visible "_isHighContrast")
		(class $FullsizeAbsolute)
		(style
			(backgroundColor = "SC.Ui_styles.SERVICE_COLORS.CONTRAST_PANEL")
			(borderRadius = "XS")
		)
	)

	(block
		(bind visible "!_isHighContrast")
		(class $FullsizeAbsolute)
		(style
			(backgroundColor = "SC.Ui_styles.SERVICE_COLORS.DARK_BLUE")
			(bind alpha "_isInfotip ? 0.5 : 0.3")
			(backgroundSize = "fill")
			(borderRadius = "XS")
		)
	)

	(block
		(bind visible "_isInfotip")
		(class $FullsizeAbsolute)
		(style
			(backgroundColor = "SC.Ui_styles.SERVICE_COLORS.WHITE")
			(alpha = 0.1)
			(borderRadius = "XS")
		)
	)
)
	)

	(tf
		(bind visible "attributeMeasureText")
		(class $TextDefaultNM)
		(style
			(bind textColor "valueColor")
			(marginLeft = "XS")
			(bind alpha "measureAlpha")
		)

		(bind text "attributeMeasureText")

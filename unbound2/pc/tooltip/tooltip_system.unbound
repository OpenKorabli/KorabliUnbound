(def css $DescriptionHtmlTextStyleSheet ()
	(extends $FontDropShadowFilter)

	(marginTextLeft = -3px)
	(marginTextRight = -3px)
	(marginTextTop = -6px)
	(marginTextBottom = -6px) 

	(multiline = true)

	(styleSheet ="'
		body {
			font-family: $DefaultFont;
			leading: -2;
			font-size: 16;
			color: #EEEEEE
		}
		.bold_default {
			font-family: $DefaultFontBold;
			color: #EEEEEE
		}
		.bold_currentPlayerRelated {
			font-family: $DefaultFontBold;
			color: #FFD966
		}
		.bold_error {
			font-family: $DefaultFontBold;
			color: #FF6600
		}
		.bold_warning {
			font-family: $DefaultFontBold;
			color: #FF9900
		}
		.default {}
		.currentPlayerRelated {
			font-family: $DefaultFont;
			color: #FFD966
		}
		.error {
			font-family: $DefaultFont;
			color: #FF6600
		}
		.warning {
			font-family: $DefaultFont;
			color: #FF9900
		}
		.p_spacing {
			font-size: 6.5px;
		}
	'")
)


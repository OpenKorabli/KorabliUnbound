(def element TooltipSystemHeaderSubheaderTextCrewOnly (_headerText:str, _subheaderText:str=null, _crewRarity:number=null)
	(scope
		(var isCrewLegendary:bool =	"_crewRarity == SC.Common.UNIFIED_ITEM_RARITY.LEGENDARY")
		(var isCrewCommon:bool =	"_crewRarity == SC.Common.UNIFIED_ITEM_RARITY.COMMON")

		(var crewStarIcon:str = "isCrewCommon		? 'common.png' :
								 isCrewLegendary	? 'unique.png'
													: 'advanced.png'")
	)
	(style (width = 100%))

	(hblock
		(style (width = 100%))

		(block
			(bind visible "!isCrewCommon && _crewRarity != null")

			(style
				(width = 27px)
				(height = 27px)
				(marginLeft = -9px)
				(marginBottom = -10px)
				(bind marginTop "-S")
				(bind marginRight "-XS")
				(bind backgroundImage "'url:../crew_commander/icons/' + crewStarIcon")
			)
		)

		(tf
			(name = 'tooltipSystemHeader')
			(class $TextDefaultBold20NM)

			(style
				(width = 100%)
				(leading = -4)
				(bind alpha "isCrewLegendary ? 1 : TA")
				(bind textColor "isCrewLegendary ? SC.Ui_styles.SERVICE_COLORS.YELLOW : SC.Ui_styles.SERVICE_COLORS.WHITE")
			)

			(bind text "_headerText")
		)
	)

	(tf
		(bind visible "_subheaderText != null")
		(class $TextDefaultNM)
		(style
			(width = 100%)
			(marginTop = "S")
			(leading = -2)
			(alpha = "TC")
		)

		(bind text "_subheaderText")
	)
)


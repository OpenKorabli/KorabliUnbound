(def element BattleStatsDetailedReportRowFloatValue (_value:number = 0, _fontClass:str = '$TextDefaultNM')
	(scope
		(var pickedPowerupsCount:number = "floor(_value) - _value < 0 ? formatSeparator(_value) : formatSeparator(_value, 0)")
	)


(def element DetailedReportSection ()
	(scope
		(event evAddedToStage)
		(event evStatsLoaded)
		(event evHideLoadingDataMessage)

		(var isContentShown:bool = false)
		(bind isContentShown "true" init=false (event "evAddedToStage"))
	)

	(dispatch evAddedToStage			dir="EventDirection.NONE"	delay=0.15		on='addedToStage')
	(dispatch evHideLoadingDataMessage	dir="EventDirection.DOWN"					(event "evStatsLoaded"))

	(class $FullsizeAbsolute)

	(block
		(style (width = 100%))

		(controller $Instance renderer='RealDetailedReportSection'
			(bind enabled "isContentShown")
		)
	)

	(block
		(class $MiddleVHAbsolutely)

		(element PBSLoadingDataCaption
			_isContentShown = "isContentShown"
		)
	)
)

(def element RealDetailedReportSection ()
	(scope
		(macro PBS_LOADING_ANIMATION_SCOPE)

		(var battleResultEntity:dhEntity =			"getSingleEntity(CC.battleResultsStructured)")
		(var battleResultsStructured:dhComponent =	"battleResultEntity.battleResultsStructured")

		(var shipsInteractionsEntity:dhEntity =		"getEntity(battleResultsStructured.interactions[0])")
		(var buildingInteractionsEntity:dhEntity =	"getEntity(battleResultsStructured.buildingInteractions[0])")
		(var hasBattleStatsCarousel:bool =			"(shipsInteractionsEntity.battleResultsEntry.children.length + buildingInteractionsEntity.battleResultsEntry.children.length) > 0")
	)

	(macro PBS_LOADING_ANIMATION)
	(style
		(width = 100%)
		(height = "{ 720: 600px, 1080: 738px }")
	)

	(block
		(style
			(width = 100%)
			(bind marginBottom "hasBattleStatsCarousel ? M : 0px")
		)

		(controller $Instance renderer='BattleStatsInteractionsCarousel'
			(bind enabled "hasBattleStatsCarousel")
		)
	)

	(scrollArea
		(class $Fullsize)

		(macro DEFAULT_VERTICAL_SCROLL_PARAMS)

		(content
			(class $Fullsize)
			(hblock
				(style
					(width = 100%)
					(gap = "XS")
				)

				(element BattleStatsDetailedReportColumn
					_list = "battleResultsStructured.causedDamage"
				)

				(element BattleStatsDetailedReportColumn
					_list = "battleResultsStructured.generalInfo"
				)

				(element BattleStatsDetailedReportColumn
					_list = "battleResultsStructured.takenDamage"
				)
			)
		)
	)
)

(def element BattleStatsDetailedReportColumn (_list:array = [])
	(style
		(width = "{ 1280: 357px, 1920: 471px }")
		(backgroundColor = "SC.Ui_styles.SERVICE_COLORS.INNER_PANEL")
	)

	(controller $Repeat renderer='BattleStatsDetailedReportRow'
		(bind count "_list.length")
		(args
			_entityId = "_list[$index]"
		)
	)
)

(def element BattleStatsDetailedReportRow (_entityId:number = 0, _childrenLength:number = 0)
	(scope
		(var battleResultEntity:dhEntity =		"getEntity(_entityId)")
		(var battleResultsEntry:dhComponent =	"battleResultEntity.battleResultsEntry")
		(var childrenCount:number =				"battleResultsEntry.children.length")

		(var rendererType:str =		"battleResultsEntry.type ?: ''")
		(var renderer:str =			"rendererType in BATTLE_STATS_DETAILED_REPORT_ROW_RENDERER ? BATTLE_STATS_DETAILED_REPORT_ROW_RENDERER[rendererType] : ''")
	)

	(style
		(width = 100%)
		(bind marginTop		"$index == 0 && isIn(rendererType, [SC.Result_screen.TYPES.LINE, SC.Result_screen.TYPES.SUBLINE])	? 5px :
							 $index == 0 && (rendererType == SC.Result_screen.TYPES.PLAIN_LINE)									? 5px
																																: 0px")
		(bind marginBottom	"rendererType == SC.Result_screen.TYPES.SUBLINE && (($index + 1) == _childrenLength)							? S :
							 rendererType == SC.Result_screen.TYPES.LINE && (($index + 1) == _childrenLength) && !(toBool(childrenCount))	? S :
							 rendererType == SC.Result_screen.TYPES.PLAIN_LINE && (($index + 1) == _childrenLength)							? S
																																			: 0px")
	)

	(controller $Instance
		(bind enabled "renderer")
		(bind renderer "renderer")
		(args
			_entityId =			"battleResultEntity.id"
			_childrenLength =	"_childrenLength"
		)
	)
)


(def element BattleStatsDetailedReportRowItem (_entityId:number = 0, _labelClass:str = '')
	(scope
		(var battleResultsStructuredEntity:dhEntity =	"getSingleEntity(CC.battleResultsStructured)")
		(var dataComponent:dhComponent =				"battleResultsStructuredEntity.dataComponent")
		(var abuseReasons:array =						"dataComponent.data['personal']['player']['abuse_components'] ?: []")

		(var battleResultsEntryEntity:dhEntity =	"getEntity(_entityId)")
		(var battleResultsEntry:dhComponent =		"battleResultsEntryEntity.battleResultsEntry")
		(var battleResultsEntryValues:array =		"battleResultsEntry.values")
		(var tooltipType:str =						"battleResultsEntry.tooltipType")
		(var color:number =							"battleResultsEntry.color in RESULT_SCREEN_ROW_FONT_COLOR	? RESULT_SCREEN_ROW_FONT_COLOR[battleResultsEntry.color]
																												: SC.Ui_styles.SERVICE_COLORS.WHITE")
		(var rowAlpha:number =						"battleResultsEntryValues[0] == null || (battleResultsEntryValues[0].value ||
																							 battleResultsEntryValues[1].value ||
																							 battleResultsEntryValues[2].value)		? 1 : 0.5")

		(var tooltipRenderer:str =	"tooltipType in BATTLE_STATS_DETAILED_REPORT_ROW_ITEM_TOOLTIP ? BATTLE_STATS_DETAILED_REPORT_ROW_ITEM_TOOLTIP[tooltipType] : ''")
	)

	(style
		(width = 100%)
		(bind alpha "rowAlpha")
	)

	(block
		(class $FullsizeAbsolute)
		(style (backgroundColor = "NO_COLOR"))

		(controller $Tooltip
			(bind enabled "toBool(battleResultsEntry.hint)")
			(renderer='SimpleStatusTooltip')
			(args
				_text = "battleResultsEntry.hint"
			)
			(macro DEFAULT_TOOLTIP_BEHAVIOUR "0")
		)

		(controller $Tooltip
			(bind enabled "toBool(tooltipRenderer)")
			(bind renderer "tooltipRenderer")
			(args
				_entityId =				"_entityId"
				_abuseStatusPBS =		"battleResultsEntryValues[2].value"
				_abuseReasonsPBS =		"abuseReasons"
				_battlesToCleanPBS =	"battleResultsEntryValues[1].value"
			)
			(macro DEFAULT_TOOLTIP_BEHAVIOUR "1")
		)
	)

	(block
		(class $InfoBlockHorizontalIndent)

		(tf
			(bind class "_labelClass")
			(style
				(hitTest = false)
				(bind textColor "color")
				(bind alpha "color == SC.Ui_styles.SERVICE_COLORS.WHITE ? TA : 1")
			)

			(bind text "battleResultsEntry.title")
		)

		(block
			(style
				(position = "absolute")
				(right = 0px)
				(flow = "reverse_horizontal")
			)

			(controller $Repeat renderer='BattleStatsDetailedReportRowValue'
				(bind count "battleResultsEntryValues.length")
				(args
					_type =			"battleResultsEntryValues[$index].type"
					_hint =			"battleResultsEntryValues[$index].hint"
					_value =		"toNumber(battleResultsEntryValues[$index].value) ?: 0"
					_fontClass =	"_labelClass"
				)
			)
		)
	)
)


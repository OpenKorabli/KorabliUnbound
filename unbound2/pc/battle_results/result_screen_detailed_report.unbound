(def element TakenDamageTooltip (_entityId:number = 0)
	(scope
		(var battleResultsStructured:dhComponent =	"getSingleComponent(CC.battleResultsStructured)")
		(var battleResultsEntryEntity:dhEntity =	"getEntity(_entityId)")
		(var battleResultsEntry:dhComponent =		"battleResultsEntryEntity.battleResultsEntry")
		(var tooltipType:str =						"battleResultsEntry.tooltipType")

		(var shipInteractionsList:array = "	tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_ARTILLERY		? battleResultsStructured.takenArtilleryTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_TORPEDOES		? battleResultsStructured.takenTorpedoesTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_BOMBS			? battleResultsStructured.takenBombsTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_ROCKETS			? battleResultsStructured.takenRocketsTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_SKIP_BOMBS		? battleResultsStructured.takenSkipBombsTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_AIRSUPPORT		? battleResultsStructured.takenAirsupportTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_NAVAL_MINES		? battleResultsStructured.takenNavalMinesTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_DEPTH_CHARGES	? battleResultsStructured.takenDepthChargesTooltip :
											tooltipType == SC.Result_screen.TOOLTIP_TYPES.TAKEN_OTHER			? battleResultsStructured.takenOtherTooltip
																												: battleResultsStructured.takenOtherTooltip")
	)

	(macro TOOLTIP_SYSTEM_DEFAULT_BACKGROUND)
	(style (width = "DEFAULT_TOOLTIP_WIDTH"))

	(block
		(style
			(width = 100%)
			(marginBottom = "SXS")
		)

		(controller $Repeat renderer='ShipInteractionsTooltipRow'
			(bind count "shipInteractionsList.length")
			(args
				_entityId = "shipInteractionsList[$index]"
			)
		)
	)
)

(def element BattleStatsDetailedReportRowValue (_type:str = '', _hint:str = '', _value:number = 0, _fontClass:str = '$TextDefaultNM')

		(var renderer:str = "_type in BATTLE_STATS_DETAILED_REPORT_ROW_VALUE_RENDERER ? BATTLE_STATS_DETAILED_REPORT_ROW_VALUE_RENDERER[_type] : 'BattleStatsDetailedReportRowDefaultValue'")

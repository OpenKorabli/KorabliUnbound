		(var sseTaskRewards:array =		"sseTaskComponent.rewards	?: []" (event "sseTaskComponent.evChanged"))

		(var rewards:array = "	sseEpicRewards.length	? sseEpicRewards :
								sseChainRewards.length	? sseChainRewards
